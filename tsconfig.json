{
  "compilerOptions": {
    "target": "ES2015",
    "module": "CommonJS",
    "lib": ["ScriptHost", "ES2019"],
    "allowJs": true,
    "rootDir": "src",
    "downlevelIteration": true,
    "strict": true,
    "strictPropertyInitialization": false,
    "moduleResolution": "node",
    "baseUrl": "./",
    "paths": {
      "src/*": ["src/*"],
      "kaitai-struct": ["src/localdb/kaitai/index.d.ts"]
    },
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true
  }
}
